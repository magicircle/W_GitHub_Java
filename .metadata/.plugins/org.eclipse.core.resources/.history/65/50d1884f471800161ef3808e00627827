import java.io.*;

public class Exam_08_method 
{
	public static void main(String[] ar)throws IOException
	{
		FileReader in = new FileReader("miro_01.txt");
		
		int i,j;
		int x_Cord=0;
		int y_Cord=0;
		int char_Temp;
		char[][] char_Line = new char[24][80];
		String line_String;
		
		i=0 ; j=0;
		while ( ( char_Temp=in.read() ) != -1 )
		{
			char_Line[j][i]=(char)char_Temp;
			System.out.println(char_Temp);
			i=i+1;
			
						
		}	 
		
		
		
		
		in.close();			
	
	}

}



