package org.javatutorials.eclipse.variable_01;

public class variable_ex01 {

	public static void main(String[] args) {
		// 변수연습
		int a;
		double b;
		string f1;
		f1="아침"
		string f2="입니다";
		
		a=1;
		System.out.println(a+a+a);
		a=123;
		System.out.println(a+a+a);
		
		b=12.21;
		System.out.println(b*b*b);
		
		System.out.println(f1+f2);		
		
	}

}
