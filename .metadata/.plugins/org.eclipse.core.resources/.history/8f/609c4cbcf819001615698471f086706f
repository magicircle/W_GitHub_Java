package javapractice0514;

import java.io.*;

public class keyboard003 {

	public static void main(String[] args) throws IOException {
		InputStreamReader myReader = new InputStreamReader(System.in);
		//int cTmp = myReader.read();
		//System.out.println(cTmp);

		char[] acTmp = new char[10];
		myReader.read(acTmp);
		for ( int i=0 ; i<acTmp.length ; i++){
			System.out.println(acTmp[i]);
		}
		
		int nTmp = myReader.read();
		System.out.println(nTmp);
		
		
			
	}
}
