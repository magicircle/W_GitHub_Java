package org.javatutorials.eclipse.loopex02;

public class loopex02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=1;
		int b;
		int c;
		long x,y;
		while(a<=9){
			a++;
			b=1;
			System.out.printf("구구단 연습 %d단\n",a);
			while(b<=9){
				c=a*b;
				System.out.printf("%d X %d = %d\n",a,b,c);
				b++;

			}
	
		}
	
	}
	

}
