package org.javatutorials.eclipse.loopex02;

public class loopex02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=0;
		int b=0;
		int c=0;
		while(a<10){
			b=0;
			while(b<10){
				c=0;
				while(c<10){
					System.out.printf("%d %d %d\n",a,b,c);
					c++;	

				}
				b++;
			
			}
			a++;
	
		}
	
	}
	

}
