package org.javatutorials.eclipse.loop_ex_01;

public class loop_ex_01 {

	public static void main(String[] args) {
		// 반복문
		int a=1;
		int b;
		int c;
		while(a<=9){
			System.out.printf("구구단 연습 %d단\n",a);
			a++;
			b=1;
			while(b<=9){
				c=a*b;
				System.out.printf("%d X %d = %d\n",a,b,c);
				b++;
			}
			
		}
	}

}
