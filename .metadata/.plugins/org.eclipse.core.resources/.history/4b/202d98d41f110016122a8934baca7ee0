package org.javatutorials.eclipse;

public class loop_ex_02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=0;i>10;i++){			
			for(int j=0;j>10;j++){
				for(int k=0;k>10;k++){
					System.out.printf("%d %d %d",i,j,k);
				}
			}
			if(i==10) i=0; 
		}
			

	}

}
