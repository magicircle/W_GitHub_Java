
public class Exam_02 {
	
	public static void main(String[] ar){
		int x = 128;
		long y = x;
		byte z = (byte)x;
		
		char a = 65;
		short b = (short)a;
		byte c = 100;
		short d = c;
		char e = (char)c;
		
		long f = 100L;
		float g = f;
		
		float h = 10.72f;
		long i = (long)h;
		
		System.out.println("x = " + x);
		System.out.println("y = " + y);
		System.out.println("z = " + z);
		System.out.println("g = " + g);
		System.out.println("i = " + i);
		
		
	}

}
